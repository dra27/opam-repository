authors: "David Allsopp"
bug-reports: "https://github.com/ocaml/opam-repository/issues"
homepage: "https://github.com/ocaml/opam-repository/wiki/Compilers"
license: "CC0-1.0"
maintainer: "David Allsopp <david.allsopp@metastack.com>"
synopsis: "The OCaml Num library (distributed with the compiler)"
description: """
Virtual package installed by OCaml compilers that included the Num library. The
Num library was moved out of the compiler distribution in OCaml 4.06.
"""
depends: "ocaml" {< "4.06.0"}
